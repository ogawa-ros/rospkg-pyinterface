<launch>
  <node pkg="pci_boards_driver" name="cpz3177" type="cpz3177.py" required="true" output="screen">
    <param name="~rate" value="0.001" />
    <param name="~rsw_id" value="0" />
    <param name="~all_ch_num" value="8" />
    <param name="~ch_num_li" value="[1,2,3,4,5,6,7,8]" />
    <param name="~single_diff" value="DIFF" />
    <param param="~ch1" value="{'ch':1,'range':'5V'}" />
    <param param="~ch2" value="{'ch':2,'range':'5V'}" />
    <param param="~ch3" value="{'ch':3,'range':'5V'}" />
    <param param="~ch4" value="{'ch':4,'range':'5V'}" />
    <param param="~ch5" value="{'ch':5,'range':'5V'}" />
    <param param="~ch6" value="{'ch':6,'range':'5V'}" />
    <param param="~ch7" value="{'ch':7,'range':'5V'}" />
    <param param="~ch8" value="{'ch':8,'range':'5V'}" />



  </node>
</launch>
